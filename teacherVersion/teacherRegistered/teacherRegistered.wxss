/* parentsVersion/parentsRegistered/parentsRegistered.wxss */
page{
  font-size: 30rpx;
  background-color: #f2f2f2;
  font-family:  Microsoft YaHei;
}

/* 顶部显示 */
.personal-center{
  background-color: #74cdce;
  padding: 50rpx 0;
  margin-bottom: 30rpx;
}
.personal-center-box{
  width: 90%;
  margin: 0 auto;
  display: flex;
  flex-direction:row;
  justify-content:center;
  align-items:center;
}
.personal-center-box-left{
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
}
.personal-center-box-left-img{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border:2px solid #fff;
  overflow: hidden;
  margin-right: 20rpx;
}
.personal-center-box-left-img>image{
  display: block;
  width: 100%;
  height: 100%;
}
.personal-center-box-left-info{
  font-size: 28rpx;
  color: #fff;
  font-weight: 600;
}
.personal-center-box-left-info-id{
  margin-top: 20rpx;
}
.personal-fun{
  width: 90%;
  border-radius: 10rpx;
  background-color: #fff;
  padding: 30rpx 0;
  margin: 30rpx auto 0 auto;
}
.personal-fun-box{
  width: 96%;
  margin: 0 auto;
  display: flex;
  flex-direction:row;
  justify-content:flex-start;
  align-items:center;
  flex-wrap:wrap;
}
.personal-fun-box-out{
  width: 216rpx;
  display: flex;
  justify-content:center;
}
.personal-fun-box-con{
  width: 120rpx;
  padding: 30rpx;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
}
.personal-fun-box-con-img{
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
}
.personal-fun-box-con-img>image{
  width: 100%;
  height: 100%;
  display: block;
}
.personal-fun-box-con-msg{
  font-size: 28rpx;
  font-weight: 600;
  text-align: center;
  color: #fff;
}

