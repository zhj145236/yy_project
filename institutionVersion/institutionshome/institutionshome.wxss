/* institutionVersion/institutionshome/institutionshome.wxss */
/* 首页 */
page{
  font-size: 30rpx;
  background-color: #f2f2f2;
  font-family:  Microsoft YaHei;
}
/* 占位图 */
.placeholder-place-img{
  width: 100%;
  height: 170rpx;
  margin: 30rpx 0;
  overflow: hidden;
}
.placeholder-place-img > image{
  display: block;
  width: 100%;
  height: 100%;
}

/* 地址定位 */
.search-set{
  position: absolute;
  top: 30rpx;
  z-index: 1;
  left: 0;
  width: 100%;
}
.parents-search{
  display:flex;
  justify-content: space-between;
  flex-direction:row;
  align-items:center;
  background: #fff;
  width: 90%;
  margin: auto;
  border-radius: 10rpx;
  overflow: hidden;
}
button::after{
  border-radius: 0;
  border:none;
  position: static;
  width:initial;
  height: initial;
}
.set{
  display:flex;
  justify-content: flex-start;
  height: 80rpx;
  width: 560rpx;
  align-items:center;
}
.location-img-view{
  display: flex;
  align-items:center;
  justify-content:flex-start;
  flex-direction:row;
  width: 120rpx;
  margin-right: 20rpx;
}
.search-inp{
  width: 420rpx;
  display: inline-block;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
}
.add-img{
  display: block;
  width: 100%;
  height: 100%;
}
.img-view{
  width: 40rpx;
  height: 40rpx;
  margin-left: 10rpx;
}
.text-info{
  color: #8EBBFB;
  font-size: 24rpx;
}
.banner{
  position: relative;
  height: 336rpx;
}
.banner-img{
  width:100%;
  height: 340rpx;
  position: absolute;
  left: 0;
  top: 0;
  display: block;
}
.banner-swiper{
  width: 100%;
  margin-left:auto;
  margin-right: auto;
  position: absolute;
  top: 140rpx;
  left: 50%;
  margin-left: -50%;
}
.slide-image{
  width: 100%;
  display: block;
  height: 200rpx;
}
.search-icon-inp{
  width: 100rpx;
  height: 80rpx;
  margin: 0;
  right: 0;
  border: none;
  padding: 0;
  position: static;
  border-radius: 0;
  background-color: #F8F8F8;
  display: flex;
  align-items: center;
}
.button-hover{
  background-color: #F8F8F8;
}
.search-icon-inp-img{
  width: 50rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  margin: 0 auto;
}

.search-icon-inp-img > image{
  display: block;
  width: 80%;
  height: 80%;
  margin: auto;
}
.change-icon{
  margin-left:40rpx;
  margin-right: 20rpx;
  width: 1rem;
  height: 1rem;
}

/* 高度占位框 */
.position-height{
  width: 100%;
  height: 96rpx;
}

/* 公用内容顶部 */
.near-teacher{
  margin-bottom: 30rpx;
}
.near-teacher-out{
  width: 90%;
  margin: 0 auto;
  background-color: #fff;
  border-radius: 10rpx;
  padding: 30rpx 0;
}
.near-teacher-title{
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
  width: 90%;
  margin:0 auto;
}
.near-teacher-title-left-text,.near-teacher-title-left,.near-teacher-title-right{
  display: flex;
  flex-direction:row;
  justify-content:flex-start;
  align-items:center;
}
.near{
  font-size: 30rpx;
  font-weight: 600;
  color: #69a5fb;
  margin-right: 10rpx;
}
.teacher{
  font-size: 26rpx;
  color: #FFA86B;
}
.near-teacher-title-right-img{
  width: 38rpx;
  height: 38rpx;
  margin-left: 10rpx;
}
.near-teacher-title-right-img > image{
  display: block;
  width: 100%;
  height: 100%;
}
.near-teacher-title-right-more{
  color: #a3a3a3;
  font-size: 28rpx;
}
.near-teacher-title-left-btn{
  width: 200rpx;
  background-color: #51aa38;
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  font-size: 26rpx;
  text-align: center;
  border-radius:50rpx;
  margin-left: 20rpx;
}
/* 附近教师内容 */
.near-teacher-count-img{
  width: 200rpx;
  height: 200rpx;
  border-radius: 100%;
  overflow: hidden;
}
.near-teacher-count-img > image{
  display: block;
  width: 100%;
  height: 100%;
}
.near-teacher-count{
  background-color: #f2f2f2;
  width: 90%;
  margin:30rpx auto 0 auto;
  padding: 20rpx 0;
  border-radius: 10rpx;
}
.near-teacher-count-out{
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
  width: 90%;
  margin: auto;
}
.near-teacher-count-left{
  width: 40%;
}
.near-teacher-count-right{
  width: 60%;
}
.near-teacher-count-right-change{
  width:100%;
}
.near-teacher-count-right-change > text{
  font-size: 28rpx;
  font-weight: 600;
}
.near-teacher-count-right-dec{
  font-size: 26rpx;
  color: #565656;
}

/* 附近家长 */
.institution-view{
  display: flex;
  flex-direction:row;
  flex-wrap:wrap;
  justify-content:space-between;
  align-items:center;
  width: 90%;
  margin:auto;
}
.institution{
  width: 48%;
  margin-top: 3%;
  background-color: #f2f2f2;
  border-radius: 10rpx;
  overflow: hidden;
}
.institution-con-style{
  margin-bottom: 10rpx;
}
.institution-img{
  width: 100%;
  height: 180rpx;
  overflow:hidden;
}
.institution-img > image{
  display: block;
  width: 100%;
}
.institution-con{
  width: 90%;
  margin: auto;
  color: #565656;
  font-size: 26rpx;
  padding: 20rpx 0;
}
.institution-details{
  width: 200rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #fff;
  border-radius:14rpx;
  background-color: #4F7CBC;
  margin-top: 20rpx;
}
.change-style{
  font-weight: 600;
}

/* 产品中心 */
.institution-view{
  display: flex;
  flex-direction:row;
  flex-wrap:wrap;
  justify-content:space-between;
  align-items:center;
  width: 90%;
  margin:auto;
}
.institution{
  width: 48%;
  margin-top: 3%;
  background-color: #f2f2f2;
  border-radius: 10rpx;
  overflow: hidden;
}
.institution-con-style{
  margin-bottom: 10rpx;
}
.institution-img{
  width: 100%;
  height: 180rpx;
  overflow:hidden;
}
.institution-img > image{
  display: block;
  width: 100%;
}
.institution-con{
  width: 90%;
  margin: auto;
  color: #565656;
  font-size: 26rpx;
  padding: 20rpx 0;
}
.change-style{
  font-weight: 600;
}
.price{
  color: #cd2a17;
}
.institution-more{
  font-size: 30rpx;
}

/* 招聘中心 */
/* con */
.diary-center{
  width: 90%;
  margin: 30rpx auto;
  padding: 40rpx 0;
  background-color: #fff;
  border-radius: 10rpx;
}
.diary-center-box{
  width: 90%;
  margin: auto;
}
.diary-center-title{
  border-bottom: 1px solid #d1d1d1;
  margin-bottom: 30rpx;
  display: flex;
  flex-direction:row;
  flex-wrap:nowrap;
  justify-content:space-between;
  align-items:center;
}
.diary-center-title-rigth{
  display: flex;
  flex-direction:row;
  flex-wrap:nowrap;
  justify-content:space-between;
  align-items:center;
}
.diary-center-title-left{
  font-size: 28rpx;
  font-weight: 600;
  height: 80rpx;
  line-height: 80rpx;
  color: #636363;
}
.diary-center-title-img{
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
}
.diary-center-title-img > image{
  display: block;
  width: 100%;
  height: 100%;
}
.diary-center-title-more{
  color: #cccccc;
  font-size: 26rpx;
}
.diary-center-copy{
  color: #acacac;
  font-size: 26rpx;
  margin: 30rpx 0;
}
.recruit-con{
  border-radius: 10rpx;
  background-color: #f2f2f2;
  overflow: hidden;
  margin-bottom:30rpx;
}
.recruit-con-img{
  width: 100%;
  height: 300rpx;
  overflow: hidden;
}
.recruit-con-img>image{
  width: 100%;
  height: 100%;
}
.recruit-con-des{
  width:90%;
  margin: auto;
  padding: 30rpx 0;
}
.recruit-con-des-type-title{
  font-size: 30rpx;
  color: #505050;
  margin-bottom: 10rpx;
}
.recruit-con-des-type-center{
  margin: 20rpx 0;
}
.recruit-con-des-type-center-title,.recruit-con-des-type-bom-title{
  margin-bottom: 10rpx;
}
.recruit-con-des-type-center,.recruit-con-des-type-bom{
  color: #797979;
  font-size: 28rpx;
}
.recruit-con-des-type-price{
  color: #fe9d3d;
  font-size: 26rpx;
}
.recruit-con-des-type-msg{
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  color: #969696;
}

.recruit-con-des-type-data{
  display: flex;
  align-items:center;
  justify-content:flex-start;
  flex-direction:row;
}
.recruit-con-des-type-data-img{
  width: 40rpx;
  height: 40rpx;
}
.recruit-con-des-type-data-img > image{
  display: block;
  width: 100%;
  height: 100%;
}

/* 企业中心 */
.personal-center{
  background-color: #69a5fb;
  padding: 50rpx 0;
}
.personal-center-box{
  width: 90%;
  margin: 0 auto;
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
}
.personal-center-box-left{
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
}
.personal-center-box-left-img{
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  border:2px solid #fff;
  overflow: hidden;
  margin-right: 20rpx;
}
.personal-center-box-left-img>image{
  display: block;
  width: 100%;
  height: 100%;
}
.personal-center-box-left-info{
  font-size: 26rpx;
  color: #fff;
}
.personal-center-box-left-info-id{
  margin: 10rpx 0;
}
.info-remind{
  display: flex;
  align-items: center;
  flex-direction:row;
}
.info-remind-num{
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #f2b04a;
  background-color: #fff;
  border-radius: 50%;
  margin-left: 10rpx;
}
.personal-center-box-right-img{
  width: 50rpx;
  height: 50rpx;
  border: 1px solid rgba(255, 255, 255, 0.2);
  border-radius: 50%;
  display: flex;
  align-items: center;
  margin: auto;
  background-color: rgba(0, 0, 0, 0.2);
}
.personal-center-box-right-img>image{
  display: block;
  width: 70%;
  height: 70%;
  margin: auto;
}
.personal-center-box-right-text{
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  margin-top: 10rpx;
  font-weight: 400;
}
.personal-fun{
  width: 90%;
  border-radius: 10rpx;
  background-color: #fff;
  padding: 30rpx 0;
  margin: 30rpx auto 0 auto;
}
.personal-fun-box{
  width: 90%;
  margin: 0 auto;
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
  flex-wrap:wrap;
}
.personal-fun-box-con{
  width: 120rpx;
  padding: 30rpx;
  border-radius: 10rpx;
  margin-bottom: 30rpx;
}
.personal-fun-box-con-img{
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
}
.personal-fun-box-con-img>image{
  width: 100%;
  height: 100%;
  display: block;
}
.personal-fun-box-con-msg{
  font-size: 28rpx;
  font-weight: 600;
  text-align: center;
  color: #fff;
}

/* 底部总部信息 */
.platform-info{
  font-size: 24rpx;
  font-weight: 600;
  color: #adadad;
  width:56%;
  margin: 100rpx auto 0 auto;
}
.platform-info > view{
  height: 40rpx;
  line-height: 40rpx;
}

/* 底部占位 */
.position-bottom{
  width: 100%;
  height: 100rpx;
}
