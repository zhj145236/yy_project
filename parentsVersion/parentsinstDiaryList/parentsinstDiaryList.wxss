/* parentsVersion/parentsinstDiaryList/parentsinstDiaryList.wxss */
page{
  font-size: 30rpx;
  background-color: #f2f2f2;
  font-family:  Microsoft YaHei;
}
/* 顶部 */
.registered{
  width: 90%;
  margin: 30rpx auto;
  padding: 30rpx 0;
  background-color: #fff;
  border-radius: 10rpx;
}
.registered-txt{
  width: 90%;
  margin: 0 auto;
  color: #F2B04A;
  font-weight: 600;
  text-align: left;
}

/* 日记列表 */
.diary-list{
  background-color: #fff;
  width: 90%;
  border-radius: 10rpx;
  padding: 30rpx 0;
  margin: 0 auto 20rpx auto;
}
.diary-list-con{
  width: 90%;
  margin: auto;
}
.diary-list-con{
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
}
.diary-list-left{
  display: flex;
  flex-direction:row;
  justify-content:flex-start;
  align-items:center;
  font-size: 28rpx;
  color: #292929;
}
.diary-list-time{
  color: #707070;
  font-size: 24rpx;
}
.diary-list-desc{
  width: 240rpx;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.diary-list-icon{
  width: 40rpx;
  height: 40rpx;
}
.diary-list-icon > image{
  display: block;
  width: 100%;
  height: 100%;
}
/* 尚未书写日记 */
.none-class{
  width: 90%;
  margin: auto;
  font-size: 26rpx;
  color: #a4a4a4;
  font-weight: 600;
  display: flex;
  align-items: center;
  justify-content:center;
}
.none-class-img{
  width: 60rpx;
  height: 60rpx;
  margin-right: 10rpx;
}
.none-class-img>image{
  display: block;
  width: 100%;
  height: 100%;
}
