/* 添加性别class */
.sex{
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.sex > image{
  width: 100%;
  height: 100%;
  display: block;
}
.none-class{
  width: 90%;
  margin: auto;
  font-size: 26rpx;
  color: #a4a4a4;
  font-weight: 600;
  display: flex;
  align-items: center;
}
.none-class-img{
  width: 60rpx;
  height: 60rpx;
  margin-right: 10rpx;
}
.none-class-img>image{
  display: block;
  width: 100%;
  height: 100%;
}
/* 底部总部信息 */
.platform-info{
  font-size: 24rpx;
  font-weight: 600;
  color: #adadad;
  width:56%;
  margin: 100rpx auto 0 auto;
}
.platform-info > view{
  height: 40rpx;
  line-height: 40rpx;
}

/* 底部占位 */
.position-bottom{
  width: 100%;
  height: 100rpx;
}
.new-abstract{
  width: 100%;
}

/* 优惠券 */
.coupons{
  background-color: #cd2a17;
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
  width: 100%;
  margin: 30rpx auto;
}
.coupons-border{
  width: 8rpx;
  background-color: #fff;
  border-left: 8rpx dashed #cd2a17;
  height: 180rpx;
}
.coupons-box{
  background-color: #FF9700;
  width: 492rpx;
  margin-left:20rpx;
}
.coupons-left{
  display: flex;
  flex-direction:row;
  justify-content:center;
  align-items:center;
  width: 90%;
  margin: 0 auto;
  padding:10rpx 0;
}
/* .coupons-left-box-price{
  display: flex;
  flex-direction:row;
  justify-content:center;
  align-items:flex-end;
} */
.coupons-left-box-price-bj{
  font-size: 28rpx;
  font-weight: 600;
  color: #cd2a17;
}
.coupons-left-box-price-num{
  font-size:82rpx;
  font-weight: 600;
  color: #cd2a17;
  text-shadow: 1px 0 0 #FFF;
}
.coupons-left-box-mes{
  margin-left: 10rpx;
}
.coupons-left-box-mes-msg{
  font-size: 30rpx;
  font-weight: 800;
  height: 40rpx;
  line-height: 40rpx;
  border-bottom: 1px solid #cd2a17;
  color: #cd2a17;
  text-align: center;
  text-shadow: 1px 0 0 #FFF;
}
.coupons-left-box-mes-type{
  font-size: 26rpx;
  font-weight: 400;
  color: #cd2a17;
  text-align: center;
  text-shadow: 1px 0 0 #FFF;
}
.coupons-right{
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
}
.coupons-right-text{
  width: 30rpx;
  margin-right:26rpx;
  font-weight: 400;
  font-size: 24rpx;
  color: #fff;
}
.helpful-hints{
  font-size: 22rpx;
  color: #e19e9e;
}

.none-class-coupons{
  width: 100%;
  margin: 20rpx 0;
}