/* pages/teacherProductCenter/teacherProductCenter.wxss */
page{
  font-size: 30rpx;
  background-color: #f2f2f2;
  font-family:  Microsoft YaHei;
  overflow-x: hidden;
}
.banner-box{
  width: 90%;
  overflow: hidden;
  border-radius: 10rpx;
}
.slide-image{
  width: 100%;
  display: block;
  height: 200rpx;
}
/* 平台核查 */
.platform-inspect{
  width: 90%;
  background-color: #fff;
  margin: 30rpx auto;
  border-radius: 10rpx;
  padding: 30rpx 0;
}
.condition-screen-top{
  display: flex;
  flex-direction:row;
  flex-wrap:wrap;
  justify-content:center;
  align-items:flex-start;
  margin: 0 auto;
}
.teacher-security-right{
  display: flex;
  flex-direction:row;
  flex-wrap:wrap;
  justify-content:flex-start;
  align-items:center;
  width: 460rpx;
}
.teacher-security{
  font-size: 26rpx;
  color: #007bd0;
  margin-right: 20rpx;
  width: 110rpx;
}
.teacher-security-box{
  color: #007bd0;
  font-size: 24rpx;
  display: flex;
  align-items:center;
  justify-content:center;
  flex-direction:row;
  margin-right: 20rpx;
  margin-bottom: 10rpx;
}
.teacher-security-box-img{
  width: 24rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.teacher-security-box-img > image{
  display: block;
  width: 100%;
  height: 100%;
}

/* 信息简介 */
.info-abstract{
  width: 90%;
  background-color: #fff;
  margin: 0 auto;
  padding: 30rpx 0;
  border-radius: 10rpx;
}
.info-abstract-box{
  width: 90%;
  margin: 0 auto;
  font-size: 26rpx;
}
.info-abstract-box-top{
  display: flex;
  justify-content:flex-start;
  align-items:flex-end;
  margin-bottom: 30rpx;
}
.info-abstract-box-top-message{
  font-size: 30rpx;
  font-weight: 600;
  color: #007bd0;
  margin-right: 10rpx;
}
.info-abstract-box-top-abstract{
  font-size: 26rpx;
  color:#cd2a17;
}
.info-abstract-box-info-data{
  display: flex;
  margin-bottom: 20rpx;
  align-items: center;
}
.info-abstract-box-info-data-totle{
  color: #575757;
  margin-right: 10rpx;
}
.info-abstract-box-info-data-con{
  color: #a5a5a5;
}
.helpful-hints{
  font-size: 22rpx;
  color: #e19e9e;
}
/* 占位图 */
.placeholder-place-img{
  width: 100%;
  height: 170rpx;
  margin: 30rpx 0;
  overflow: hidden;
}
.placeholder-place-img > image{
  display: block;
  width: 100%;
  height: 100%;
}

/* 产品中心 */
.near-teacher-out{
  width: 90%;
  margin: 0 auto;
  background-color: #fff;
  border-radius: 10rpx;
  padding: 30rpx 0;
}
.near-teacher-title{
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
  width: 90%;
  margin:0 auto 20rpx auto;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #c7c7c7;
}
.near-teacher-title-left-text,.near-teacher-title-left,.near-teacher-title-right{
  display: flex;
  flex-direction:row;
  justify-content:flex-start;
  align-items:center;
}
.near{
  font-size: 30rpx;
  font-weight: 600;
  color: #007bd0;
  margin-right: 10rpx;
}
.teacher{
  font-size: 26rpx;
  color: #cd2a17;
}
.near-teacher-title-right-img{
  width: 38rpx;
  height: 38rpx;
  margin-left: 10rpx;
}
.near-teacher-title-right-img > image{
  display: block;
  width: 100%;
  height: 100%;
}
.near-teacher-title-right-more{
  color: #a3a3a3;
  font-size: 28rpx;
}


.institution-view{
  display: flex;
  flex-direction:row;
  flex-wrap:wrap;
  justify-content:space-between;
  align-items:center;
}
.change-style{
  font-weight: 600;
}
.near-teacher-count-img{
  width: 160rpx;
  height: 160rpx;
  border-radius: 100%;
  overflow: hidden;
  background-color: #f7f7f7;
}
.near-teacher-count-img > image{
  display: block;
  width: 100%;
  height: 100%;
}
.near-teacher-count-box{
  width: 90%;
  margin: auto;
}
.near-teacher-count{
  background-color: #f2f2f2;
  margin:30rpx auto 0 auto;
  padding: 20rpx 0;
  border-radius: 10rpx;
}
.near-teacher-count-out{
  display: flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
  width: 90%;
  margin: auto;
}
.near-teacher-count-left{
  width: 40%;
}
.near-teacher-count-right{
  width: 60%;
}
.near-teacher-count-right-change{
  width:100%;
}
.near-teacher-count-right-change > text{
  font-size: 28rpx;
  font-weight: 600;
}
.near-teacher-count-right-dec{
  font-size: 26rpx;
  color: #565656;
}
.near-teacher-count-right-change-txt{
  margin-bottom: 10rpx;
}
.new-box{
  font-size: 26rpx;
  margin-bottom: 10rpx;
}
.new-box-sex,.new-box-price,.new-box-knob{
  display: flex;
  align-items: center;
}
.title-box{
  color: #464646;
  font-weight: 600;
  font-size: 30rpx;
}
.new-box-nature{
  margin-left: 20rpx;
  font-size: 24rpx;
  color: #565656;
  font-weight: normal;
}
.yet-new-box-num{
  color: #FF9700;
}
.new-box-price{
  color: #cd2a17;
  font-weight: 600;
  font-size: 28rpx;
}
.new-box-price > text{
  font-size: 24rpx;
}

/* 资质证书 */
.certification{
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
}
.certification > image{
  display: block;
  width: 100%;
  height: 100%;
}
.certificate{
  width:100%;
  margin-bottom: 30rpx;
}
.certificate > image{
  display: block;
  width:100%;
  height: 100%;
}

/* 人生格言 */
.other-style{
  align-items:flex-start;
}
.change-style{
  width: 210rpx;
}
.change-width{
  width: 460rpx;
}

/* 给他/她留言 */
.leave-word-box{
  width: 100%;
  margin: 0 auto 30rpx auto;
  background-color: #f6f6f6;
  border-radius: 10rpx;
  padding-top: 30rpx;
}
.leave-word{
  width: 90%;
  margin: 0 auto;
  /* text-indent:30rpx */
}
.leave-word-btn-box{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-top: 1px solid #e5e5e5;
  text-align: right;
  display: flex;
  justify-content:flex-end;
  flex-direction:row-reverse;
  align-items: center;
}
.leave-word-btn{
  display: block;
  border: none;
  background-color: #e5e5e5;
  width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  padding: 0;
  position: static;
  margin-right: 10rpx;
  border-radius: 0;
  color: #979797;
}
.pad-btn{
  padding-bottom: 30rpx;
}
.leave-word-box-info{
  width: 90%;
  margin:auto;
}
.leave-word-info-name{
  color: #cd2a17;
  margin: 30rpx 0 10rpx 0;
}
.leave-word-info-con{
  color:#414141;
}
.leave-word-info-name-parents{
  color: #007bd0;
}
.leave-word-info-name-symbol{
  color: #2c2c2c;
}
button::after{
  border: none;
  width: 0;
  height: 0;
  position: static;
}

